#! /usr/bin/env node
var process = require('process');
var util = require('../dist/experiment-ast.cjs.js');
var argv = process.argv;
var cmd = argv[2];
var src = argv[3];
var dist = argv[4];

// 操作处理
if (cmd === 'handleVueFileLess') {
    util.handleVueFileLess(src, dist);
} else if (cmd === 'handleJsRouter') {
    util.handleJsRouter(src, dist);
} else if (cmd === 'handleJsStore') {
    util.handleJsStore(src, dist);
} else if (cmd === 'handleJsVue') {
    util.handleJsVue(src, dist);
} else if (cmd === 'handleTsClassVue') {
    util.handleTsClassVue(src, dist);
} else if (cmd === 'handleLessCode') {
    util.handleLessCode(src, dist);
} else if (cmd === 'handleLessFile') {
    util.handleLessFile(src, dist);
}